<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.concierge">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconapp"
        android:label="@string/app_name"
        android:theme="@style/Theme.Concierge"
        android:name=".App">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ChatActivity"
            android:exported="false" />
        <activity
            android:name=".FoodDrinksActivity"
            android:exported="false" />
        <activity
            android:name=".SpaActivity"
            android:exported="false" />
        <activity
            android:name=".TransferActivity"
            android:exported="false" />
        <activity
            android:name=".HouseholdActivity"
            android:exported="false" />
        <activity
            android:name=".ConciergeChatActivity"
            android:exported="false" />
        <activity
            android:name=".ConciergeDashboardActivity"
            android:exported="false" />

    </application>

</manifest>